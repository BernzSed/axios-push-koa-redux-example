{
  "name": "koa-axios-ssr-push-example",
  "version": "1.0.0",
  "repository": "",
  "description": "An example of how to use axios-isomorphic-push with koa, react, and redux",
  "main": "index.js",
  "scripts": {
    "start": "npm run build:dev; npm run serve",
    "clean": "rm -rf assets/bundle.js",
    "serve": "NODE_PATH=$NODE_PATH:./shared nodemon --expose-http2 --harmony .",
    "debug": "NODE_PATH=$NODE_PATH:./shared node debug --expose-http2 --harmony .",
    "build:dev": "NODE_ENV=development webpack --progress --color -p --config webpack.config.js",
    "build:prod": "NODE_ENV=production webpack --progress --color -p --config webpack.config.js"
  },
  "engines": {
    "node": ">=8.5.0"
  },
  "author": "Bernie Stern <bernzs@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.2",
    "axios-isomorphic-push": "file:../axios-isomorphic-push",
    "delay": "^2.0.0",
    "immutable": "^3.8.1",
    "koa": "^2.3.0",
    "koa-mount": "^3.0.0",
    "koa-router": "^7.2.1",
    "koa-static": "^4.0.1",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.7.2",
    "eslint-plugin-react": "^7.4.0",
    "nodemon": "^1.12.1",
    "webpack": "^3.6.0"
  }
}
